{
  "builders": [
    {
      "type": "virtualbox-ovf",
      "source_path": "/home/nir0s/.vagrant.d/boxes/precise64/0/virtualbox/box.ovf",
      "vm_name": "cloudify",
      "ssh_username": "vagrant",
      "ssh_key_path": "/home/nir0s/.vagrant.d/insecure_private_key",
      "ssh_wait_timeout": "3m",
      "shutdown_command": "echo 'packer' | sudo -S shutdown -P now",
      "vboxmanage": [
        ["modifyvm", "{{.Name}}", "--memory", "2048"],
        ["modifyvm", "{{.Name}}", "--cpus", "2"],
        ["modifyvm", "{{.Name}}", "--natdnshostresolver1", "on"]
      ]
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "only": ["virtualbox-ovf"],
      "inline": ["mkdir -p /home/vagrant/.ssh/"]
    },
    {
      "type": "file",
      "only": ["virtualbox-ovf"],
      "source": "insecure_private_key",
      "destination": "/home/vagrant/.ssh/insecure_private_key"
    },
    {
      "type": "shell",
      "script": "provision.sh"
    }
  ],
  "post-processors": ["vagrant"]
}